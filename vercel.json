{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node",
      "config": { "maxLambdaSize": "10mb" }
    }
  ],
  "routes": [
    {
      "src": "/(.*)\\.(woff|woff2|eot|ttf)",
      "headers": {
        "Content-Type": "font/woff2" // 폰트 파일에 대한 MIME 유형 설정
      },
      "continue": true
    },
    {
      "src": "/.*",
      "dest": "/"
    }
  ]
}
