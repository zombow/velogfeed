{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node",
      "config": { "maxLambdaSize": "10mb" }
    }
  ],
  "routes": [
    {
      "src": "/fonts/.*",
      "headers": { "Content-Type": "application/font-woff" },
      "dest": "/path/to/your/fonts"
    },
    {
      "src": "/.*",
      "dest": "/"
    }
  ]
}
